          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["modules/http/tsconfig.lib.json", "modules/http/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
