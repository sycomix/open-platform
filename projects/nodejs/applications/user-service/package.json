{
  "engines": {
    "node": "10.16.0"
  },
  "license": "Apache-2.0",
  "name": "@tenlastic/user-service",
  "version": "0.0.0",
  "scripts": {
    "docker": "../../scripts/docker-compose.sh",
    "documentation": "merge-yaml -i swagger.yml src/**/swagger.yml -o ./swagger-output.yml",
    "lint": "../../scripts/lint.sh",
    "migrations": "migrate-mongo up -f ../../migrate-mongo-config.js",
    "migrations:create": "migrate-mongo create -f ../../migrate-mongo-config.js",
    "migrations:down": "migrate-mongo down -f ../../migrate-mongo-config.js",
    "migrations:status": "migrate-mongo status -f ../../migrate-mongo-config.js",
    "start": "../../scripts/start.sh",
    "test": "../../scripts/test.sh"
  },
  "dependencies": {
    "@tenlastic/api-module": "0.0.0",
    "@tenlastic/validations-module": "0.0.0",
    "bcrypt": "3.0.6",
    "jsonwebtoken": "8.4.0",
    "koa": "2.7.0",
    "koa-router": "7.4.0",
    "mongoose": "5.4.15",
    "source-map-support": "0.5.12",
    "typegoose": "5.6.0"
  },
  "devDependencies": {
    "migrate-mongo": "6.0.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  }
}
